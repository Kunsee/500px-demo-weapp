.container {
  align-items:flex-start;
}

.search-bar{
    width:100%;
    /*position: fixed;*/
    /* margin: 20rpx; */
    height:100rpx;
    top:0;
    background: #ffffff;
    align-items: center;
    justify-content: center;
    display: flex;
}

.search-bar-warper{
    background:#eeeeee;
    height:60rpx;
    margin:0 20rpx;
    border-radius: 10rpx;
    padding: 0 20rpx;
    align-items: center;
    display: flex;
    width:90%;
}
.search-bar-warper input{
    width:90%;
}

.photo-list{
  width:100%;
  display:flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
}

.photo-item {
  display: flex;
  flex-direction: row;
  padding:2rpx 0rpx;
}

.photo{
  width:373rpx;
  height:373rpx;
  /* border-radius: 5rpx; */
  background: #ffffff;
}
